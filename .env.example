# Configuración de APIs para bases de datos jurídicas

# Bases de datos públicas (generalmente no requieren API key)
CENDOJ_BASE_URL=https://www.poderjudicial.es/search/indexAN.jsp
BOE_BASE_URL=https://www.boe.es/buscar/act.php
EUR_LEX_BASE_URL=https://eur-lex.europa.eu/search.html

# Bases de datos comerciales (requieren suscripción y API keys)
# Aranzadi Digital (LA LEY)
VITE_ARANZADI_API_KEY=your_aranzadi_api_key_here
VITE_ARANZADI_BASE_URL=https://insignis.aranzadidigital.es

# La Ley Digital (Wolters Kluwer)
VITE_LALEY_API_KEY=your_laley_api_key_here
VITE_LALEY_BASE_URL=https://laleydigital.wolterskluwer.es

# vLex
VITE_VLEX_API_KEY=your_vlex_api_key_here
VITE_VLEX_BASE_URL=https://vlex.es/search

# Iustel
VITE_IUSTEL_API_KEY=your_iustel_api_key_here
VITE_IUSTEL_BASE_URL=https://www.iustel.com

# Configuración de caché
VITE_CACHE_ENABLED=true
VITE_CACHE_DURATION=3600000

# Configuración de búsqueda
VITE_MAX_RESULTS_PER_SOURCE=50
VITE_SEARCH_TIMEOUT=30000

# Configuración de rate limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=3600000